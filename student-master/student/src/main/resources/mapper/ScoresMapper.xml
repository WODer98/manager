    <!--查询全部学生成绩-->
    <select id="selectStudentScores" resultMap="studentScores">
    SELECT student.student_account,student.student_name,courses.Course_id,courses.Course_name,scores.Score
    FROM student,courses,scores
    WHERE student.student_account = scores.Student_account AND courses.Course_id = scores.Course_id
  </select>

    <!--各科平均成绩查询-->
    <select id="selectAvgScoresSubject" resultMap="avgScoresSubject">
           SELECT courses.Course_id,courses.Course_name,ROUND(AVG(scores.Score),0) avgScore
        FROM courses,scores
        WHERE courses.Course_id = scores.Course_id
		GROUP BY Course_id
  </select>

